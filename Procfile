web: java -Dspring.profiles.active=${SPRING_PROFILES_ACTIVE} -Dserver.port=${PORT} -Dspring.datasource.driver-class-name=${DRIVER_CLASS} -Dspring.datasource.username=${DATABASE_USERNAME} -Dspring.datasource.password=${DATABASE_PASSWORD} -Dspring.datasource.url=${DATABASE_URL} -Dspring.redis.host=${REDIS_HOST} -Dspring.redis.port=${REDIS_PORT} -Dspring.redis.password=${REDIS_PASSWORD} -Dspring.mail.host=${MAIL_HOST} -Dspring.mail.username=${MAIL_USERNAME} -Dspring.mail.password=${MAIL_PASSWORD} -Dspring.mail.default-encoding=${ENCODING} -Dcors.url=${CORS_URL} -Dmicroservice.peopleservice.url=${PEOPLE_SERVICE_URL} -jar build/libs/peopleservice-0.0.1-SNAPSHOT.jar